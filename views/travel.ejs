                            var adult_rt = document.getElementById("AdultsRT").value;
                            if (adult_pass < 9) {
                                var k = parseInt(adult_rt) + 1;
                                document.getElementById("AdultsRT").value = k;
                            }

                        }

                        function decrease_adult_rt() {
                            var adult_rt = document.getElementById("AdultsRT").value;
